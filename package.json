{
    "name": "formatting-inputs",
    "private": true,
    "version": "0.1.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "storybook": "storybook dev",
        "build-storybook": "storybook build",
        "eslint": "eslint -c eslint.config.mjs",
        "lint": "yarn -s prettier-check && yarn -s eslint",
        "prettier-check": "prettier --check --log-level warn --cache .",
        "prettier-fix": "prettier --write .",
        "test": "vitest run"
    },
    "peerDependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@eslint/js": "9.3.0",
        "@storybook/addon-essentials": "8.1.3",
        "@storybook/addon-interactions": "8.1.3",
        "@storybook/addon-links": "8.1.3",
        "@storybook/cli": "8.1.3",
        "@storybook/react": "8.1.3",
        "@storybook/react-vite": "8.1.3",
        "@storybook/testing-library": "0.2.2",
        "@trivago/prettier-plugin-sort-imports": "4.3.0",
        "@types/react": "18.3.2",
        "@types/react-dom": "18.3.0",
        "@typescript-eslint/eslint-plugin": "7.10.0",
        "@typescript-eslint/parser": "7.10.0",
        "@vitejs/plugin-react": "4.3.0",
        "eslint": "9.3.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-import": "2.29.1",
        "eslint-plugin-jsx-a11y": "6.8.0",
        "eslint-plugin-react": "7.34.1",
        "prettier": "3.2.5",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "storybook": "8.1.3",
        "typescript": "5.4.5",
        "typescript-eslint": "7.10.0",
        "vite": "5.2.11",
        "vite-plugin-dts": "3.9.1",
        "vitest": "1.6.0"
    },
    "files": [
        "dist"
    ],
    "main": "./dist/formatting-inputs.umd.js",
    "module": "./dist/formatting-inputs.es.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/formatting-inputs.es.js",
            "require": "./dist/formatting-inputs.umd.js"
        }
    },
    "engines": {
        "node": ">=18.13.0 < 21.0.0"
    },
    "resolutions": {
        "string-width": "^4",
        "strip-ansi": "^6"
    }
}
