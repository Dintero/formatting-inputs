            "import": "./dist/formatting-inputs.es.js",
            "require": "./dist/formatting-inputs.umd.js"
        }
    },
    "engines": {
        "node": ">=18.13.0 < 21.0.0"
    },
    "resolutions": {
        "string-width": "^4",
        "strip-ansi": "^6"
