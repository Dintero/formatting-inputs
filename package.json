{
    "name": "formatting-inputs",
    "private": true,
    "version": "0.1.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "storybook": "storybook dev",
        "build-storybook": "storybook build",
        "prettier": "yarn run biome format",
        "lint": "yarn -s prettier && yarn -s biome lint",
        "test": "vitest run"
    },
    "peerDependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@biomejs/biome": "1.9.4",
        "@storybook/addon-essentials": "8.5.0",
        "@storybook/addon-interactions": "8.5.0",
        "@storybook/addon-links": "8.5.0",
        "@storybook/cli": "8.5.0",
        "@storybook/react": "8.5.0",
        "@storybook/react-vite": "8.5.0",
        "@storybook/testing-library": "0.2.2",
        "@types/react": "19.0.7",
        "@types/react-dom": "19.0.3",
        "@vitejs/plugin-react": "4.3.4",
        "react": "19.0.0",
        "react-dom": "19.0.0",
        "storybook": "8.5.0",
        "typescript": "5.7.3",
        "vite": "6.0.11",
        "vite-plugin-dts": "4.5.0",
        "vitest": "3.0.3"
    },
    "files": ["dist"],
    "main": "./dist/formatting-inputs.umd.js",
    "module": "./dist/formatting-inputs.es.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/formatting-inputs.es.js",
            "require": "./dist/formatting-inputs.umd.js"
        }
    },
    "engines": {
        "node": ">=20.0.0 < 23.0.0"
    },
    "resolutions": {
        "string-width": "^4",
        "strip-ansi": "^6"
    }
